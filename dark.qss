.QWidget {
   background-color: #332C2B;
}

QWidget {
    background-color: #332C2B;
}

QPushButton {
    background-color: #332C2B;
    border-width: 2px;
    border-color: #5F5D5D;
    border-style: solid;
    border-radius: 10;
    color: #5F5D5D;
}

QPushButton#pushButton_logo {
    color: #00A2E9;
    border-radius: 10;
    background-color: #332C2B;
    border: 1px solid #474443;
    border-bottom: 0px;
    margin-left: 2px;
    margin-right: 2px;
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #222222, stop:1 #474443);
    background-image: url(:/logo.png);
    background-position: center;
    background-repeat: no-repeat;
    background-attachment: fixed;
    background-size:100% 65%;
}

QPushButton:hover {
   background-color: #332C2B;
   color: #00A2E9;
   border-color: #00A2E9;
}

QPushButton#pushButton_auto_speed {
    border-top-left-radius: 16;
}

QPushButton#pushButton_manual_speed {
    border-top-right-radius: 16;
}
QPushButton#pushButton_dust_elimination {
    border-buttom-left-radius: 16;
}

QPushButton#pushButton_pushButton_save_setting {
    border-buttom-right-radius: 16;
}

QPushButton#pushButton_lights_up {
    border-top-left-radius: 16;
    border-bottom-left-radius: 16;
}

QPushButton#pushButton_lights_off {
    border-top-right-radius: 16;
    border-bottom-right-radius: 16;
}

QPushButton#pushButton_static_light {
    border-top-left-radius: 16;
    border-bottom-left-radius: 16;
}

QPushButton#pushButton_dynamic_light {
    border-top-right-radius: 16;
    border-bottom-right-radius: 16;
}

QPushButton#Btn_ManualStartup {
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0;
}

QPushButton#Btn_AutoStartup {
    border-top-left-radius: 6;
    border-top-right-radius: 0;
    border-bottom-left-radius: 6;
    border-bottom-right-radius: 0;
}


QPushButton:pressed {
    background-color: #807876;
}



QLabel {
    border: none;
    color: #00A2E9;
}



QCheckBox {
    background-color: #332C2B;
    border-color: #00A2E9;
    color: #00A2E9;
}



QTextBrowser#graphicsView  {
    color: #332C2B;
    border: 2px solid #00A2E9;
    border-top: 0px;
    background-color: #332C2B;
}


#tabWidget QTabBar::tab{
        min-width:200px;
        max-width:200px;
        color: #00A2E9;
        background-color: #332C2B;
        border: 1px solid #474443;
        border-bottom: 0px;
        margin-left: 1px;
        margin-right: 1px;
        background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #222222, stop:1 #474443);
        border-top-left-radius: 4;
        border-top-right-radius: 4;
}

QTabWidget::pane {
     border-right: 0;
}

#tabWidget QTabBar::tab:selected{
        color: white;
}

QSlider::groove::horizontal{
    border: 1px solid #5F5D5D;
    background: #332C2B;
    height: 9px;
    border-top-left-radius: 4;
    border-top-right-radius: 4;
    border-bottom-left-radius: 4;
    border-bottom-right-radius: 4;
}


QSlider::handle:horizontal {
    border: 1px solid #5F5D5D;
    width: 18px;
    height: 20px;
    background-color: #fcfcfc;
    margin: -8px -8px -8px -8px;
    border-radius: 9;
}


QSlider::sub-page:horizontal#horizontalSlider_p1{
    background: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 #00A2E9, stop:0.25 #0298f1, stop:0.5 #0092d2, stop:1 #0280cb);
    height: 9px;
    border-bottom-left-radius: 4;
    border-top-left-radius: 4;
}

